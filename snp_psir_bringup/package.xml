  <exec_depend>pcl_ros</exec_depend>
  <exec_depend>pilz_modbus</exec_depend>
  <exec_depend>pilz_sto_modbus_adapter</exec_depend>
  <exec_depend>psir_description</exec_depend>
  <exec_depend>psir_demo_moveit_config</exec_depend>
  <exec_depend>psir_moveit_config</exec_depend>
  <exec_depend>position_controllers</exec_depend>
  <exec_depend>roslaunch</exec_depend>
  <exec_depend>rviz</exec_depend>
  <exec_depend>snp_psir_description</exec_depend>
  <exec_depend>snp_psir_support</exec_depend>
  <exec_depend>velocity_controllers</exec_depend>
  <exec_depend>xacro</exec_depend>

  <export>
    <gazebo_ros
        gazebo_model_path="${prefix}/models" />
  </export>
</package>
