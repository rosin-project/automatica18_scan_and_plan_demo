<?xml version="1.0"?>
<package format="2">
  <name>snp_psir_bringup</name>
  <version>0.0.0</version>
  <description>The snp_psir_bringup package</description>

  <maintainer email="harshavardhan.deshpande@ipa.fraunhofer.de">Harsh Deshpande</maintainer>
  <maintainer email="jonathan.hechtbauer@ipa.fraunhofer.de">Jonathan Hechtbauer</maintainer>

  <license>Apache License 2.0</license>

  <buildtool_depend>catkin</buildtool_depend>

  <exec_depend>controller_manager</exec_depend>
  <exec_depend>gazebo_ros</exec_depend>
  <exec_depend>godel_process_planning</exec_depend>
  <exec_depend>godel_surface_detection</exec_depend>
  <exec_depend>industrial_robot_simulator_service</exec_depend>
  <exec_depend>joint_state_controller</exec_depend>
  <exec_depend>joint_trajectory_controller</exec_depend>
